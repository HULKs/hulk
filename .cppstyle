naming:
    classes: ^([A-Z][a-z0-9]*)+$
    variables: ^([a-z0-9]+)([A-Z][a-z0-9]*)*$
    members:
        private: ^([a-z0-9]+)([A-Z][a-z0-9]*)*_$
        protected: ^([a-z0-9]+)([A-Z][a-z0-9]*)*_$
        public: ^([a-z0-9]+)([A-Z][a-z0-9]*)*$
    functions: ^([a-z0-9]+)([A-Z][a-z0-9]*)*$
    methods: ^([a-z0-9]+)([A-Z][a-z0-9]*)*$
order:
    access_specifier_required: True
    access_specifier:
        - "public"
        - "protected"
        - "private"
